<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>sinpkmnms</application>
    <version>2012-0708-2323</version>
    
    <precompilation-enabled>
        true
    </precompilation-enabled>
    
    <system-properties>
        <property name="slim3.hotReloading" value="false"/>
        <property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties"/>
    </system-properties>
    
    <static-files>
        <include path="/**.html"/>
        <include path="/**.css"/>
        <include path="/**.js"/>
        <include path="/**.png"/>
        <include path="/**.gif"/>
        <include path="/**.jpg"/>
        <include path="/favicon.ico"/>
        <exclude path="/lib/closure-library/**"/>
        <exclude path="soy/**"/>
        <exclude path="**_test.html"/>
        <exclude path="**/test.html"/>
        <exclude path="/**.bat"/>
    </static-files>
    
    <resource-files>
        <exclude path="/lib/closure-library/**"/>
        <exclude path="soy/**"/>
        <exclude path="**_test.html"/>
        <exclude path="**/test.html"/>
        <exclude path="/**.bat"/>
    </resource-files>
    
    <admin-console>
        <page name="appstats" url="/appstats/"/>
    </admin-console>
    <sessions-enabled>
        true
    </sessions-enabled>
    <threadsafe>
        false
    </threadsafe>
    
</appengine-web-app>
